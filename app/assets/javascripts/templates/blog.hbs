<div class="carousel no-dots" data-carousel>
  <ul class="slides">
    {{#each Ember.ENV.ASSETS.lookbooks.fall2014}}
      <li>
        <img class="banner blog-banner" src="{{unbound this}}" alt="banner">
      </li>
    {{/each}}
  </ul>
</div>

<section class="blog">
  <section class="posts">
    {{cloaked-collection cloakView="post" content=posts}}
  </section>

  <aside class="links" data-sticky>
    {{ render "links" this}}
  </aside>
</section>
